{
  "name": "gia-externe-auth0-scim",
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/auth0/scim"
  },
  "scripts": {
    "start": "node server.js",
    "test": "lab -v",
    "build": "a0-ext build:server ./webtask.js ./dist"
  },
  "auth0-extension": {
    "externals": [
      "async@2.1.2",
      "auth0@2.4.0",
      "auth0-extension-tools@1.3.1",
      "body-parser@1.12.4",
      "express@4.14.0",
      "express-jwt@5.3.1",
      "joi@9.0.4",
      "jsonpath@1.0.0",
      "jsonwebtoken@7.1.9",
      "jwks-rsa@1.1.1",
      "node-uuid@1.4.3",
      "webtask-tools"
    ],
    "nodeTarget": "8.9.0",
    "bundleModules": false,
    "useBabel": true,
    "settings": {
      "WARN_DB_SIZE": 409600,
      "MAX_MULTISELECT_USERS": 5,
      "MULTISELECT_DEBOUNCE_MS": 250,
      "PER_PAGE": 10
    }
  },
  "license": "MIT",
  "dependencies": {
    "async": "^2.6.1",
    "auth0": "^2.9.1",
    "auth0-extension-express-tools": "^1.1.7",
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "joi": "^13.3.0",
    "jsonpath": "^1.0.0",
    "jsonwebtoken": "^8.2.2",
    "jwks-rsa": "^1.2.1",
    "node-uuid": "^1.4.8",
    "webtask-tools": "^1.3.0"
  },
  "devDependencies": {
    "auth0-extensions-cli": "^1.0.9",
    "babel-loader": "7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "code": "^5.2.0",
    "hoek": "^5.0.3",
    "lab": "^15.4.5",
    "lodash": "^4.17.10",
    "morgan": "^1.9.0",
    "webpack": "^1.13.1"
  }
}
